{
  "builders": [
    {
      "CPUs": "{{user `cpu_num`}}",
      "RAM": "{{user `mem_size`}}",
      "cluster": "{{user `vsphere_compute_cluster`}}",
      "communicator": "winrm",
      "convert_to_template": "true",
      "datacenter": "{{user `vsphere_dc_name`}}",
      "datastore": "{{user `vsphere_datastore`}}",
      "disk_controller_type": "pvscsi",
      "floppy_files": [
        "setup/win22/autounattend.xml",
        "setup/vmtools.cmd"
      ],
      "folder": "{{user `vsphere_folder`}}",
      "guest_os_type": "windows2019srvNext_64Guest",
      "host": "{{user `vsphere_host`}}",
      "insecure_connection": "true",
      "iso_paths": [
        "{{user `os_iso_path`}}"
      ],
      "network_adapters": [
        {
          "network": "{{user `vsphere_portgroup_name`}}",
          "network_card": "vmxnet3"

        }
      ],
      "password": "{{user `vsphere_password`}}",
      "storage": [
        {
          "disk_size": "{{user `disk_size`}}",
          "disk_thin_provisioned": true
        }
      ],
      "type": "vsphere-iso",
      "username": "{{user `vsphere_user`}}",
      "vcenter_server": "{{user `vsphere_server`}}",
      "vm_name": "{{user `vsphere_template_name`}}",
      "winrm_password": "{{user `winadmin_password`}}",
      "winrm_username": "Administrator"
    }
  ],
  "provisioners": [
    {
      "inline": [
        "dir c:\\"
      ],
      "type": "windows-shell"
    }
  ],
  "sensitive-variables": [
    "vsphere_password",
    "winadmin_password"
  ]
}